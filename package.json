{
	"name": "iscode",
	"displayName": "ISCode",
	"description": "Support ISCode",
	"version": "0.1.2",
	"publisher": "ProfesseurIssou",
	"repository": {
		"url": "https://github.com/ProfesseurIssou/iscode"
	},
	"engines": {
		"vscode": "^1.60.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:iscode.openFolder",
		"onCommand:iscode.translate",
		"onStartupFinished"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "iscode.openFolder",
				"title": "ISCode : Open translate folder"
			},
			{
				"command": "iscode.translate",
				"title": "ISCode : Translate code"
			}
		],
		"keybindings": [
			{
				"command": "iscode.translate",
				"key": "f5"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile && npm run lint",
		"lint": "eslint src --ext ts",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/vscode": "^1.60.0",
		"@types/glob": "^7.1.3",
		"@types/mocha": "^8.2.2",
		"@types/node": "14.x",
		"eslint": "^7.27.0",
		"@typescript-eslint/eslint-plugin": "^4.26.0",
		"@typescript-eslint/parser": "^4.26.0",
		"glob": "^7.1.7",
		"mocha": "^8.4.0",
		"typescript": "^4.3.2",
		"vscode-test": "^1.5.2"
	}
}
